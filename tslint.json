{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "member-access": [true, "no-public"],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-first",
        "named-imports-order": "lowercase-last",
        "grouped-imports": true
      }
    ],
    "no-submodule-imports": false
  },
  "rulesDirectory": []
}
